  },
  "dependencies": {
    "secure-id": "^0.1.1"
  },
  "devDependencies": {
    "typescript": "^4.0.3",
    "jest": "26.5.2",
    "ts-jest": "^26.4.1",
    "@types/jest": "^26.0.14",
    "rimraf": "^3.0.2",
    "npm-run-all": "^4.1.5"
