{
  "name": "safe-id",
  "version": "0.1.1",
  "description": "Secure and URL safe id for Browser, Node.js, and ReactNative written in TypeScript.",
  "author": {
    "name": "Yuhsak Inoue",
    "email": "yuhsak.inoue@gmail.com",
    "url": "https://github.com/Yuhsak"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
